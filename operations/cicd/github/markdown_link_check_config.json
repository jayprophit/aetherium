{
  "ignorePatterns": [
    {
      "pattern": "^https?://localhost"
    },
    {
      "pattern": "^https?://127.0.0.1"
    },
    {
      "pattern": "^#"
    },
    {
      "pattern": "^mailto:"
    },
    {
      "pattern": "^https?://github\.com/.*/issues/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://yourusername.github.io/knowledge-base/"
    },
    {
      "pattern": "^docs/",
      "replacement": "https://yourusername.github.io/knowledge-base/"
    }
  ],
  "aliveStatusCodes": [200, 206, 403, 429],
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "30s",
  "timeout": "30s"
}
