# AETHERIUM AI PLATFORM - ENVIRONMENT CONFIGURATION
# Copy this file to .env.local for development or .env.production for production

# ======================
# API CONFIGURATION
# ======================
REACT_APP_API_URL=http://localhost:8000
REACT_APP_WS_URL=ws://localhost:8000/ws
REACT_APP_API_TIMEOUT=10000

# ======================
# AI MODELS CONFIGURATION  
# ======================
# OpenAI
REACT_APP_OPENAI_API_KEY=your_openai_api_key_here
REACT_APP_OPENAI_MODEL=gpt-4

# Anthropic Claude
REACT_APP_CLAUDE_API_KEY=your_claude_api_key_here
REACT_APP_CLAUDE_MODEL=claude-3-sonnet-20240229

# Google Gemini
REACT_APP_GEMINI_API_KEY=your_gemini_api_key_here
REACT_APP_GEMINI_MODEL=gemini-pro

# DeepSeek
REACT_APP_DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Qwen
REACT_APP_QWEN_API_KEY=your_qwen_api_key_here

# ======================
# AETHERIUM QUANTUM MODELS
# ======================
REACT_APP_AETHERIUM_QUANTUM_ENABLED=true
REACT_APP_AETHERIUM_NEURAL_ENABLED=true
REACT_APP_AETHERIUM_CRYSTAL_ENABLED=true

# ======================
# PLATFORM FEATURES
# ======================
REACT_APP_ENABLE_VOICE_INPUT=true
REACT_APP_ENABLE_FILE_UPLOAD=true
REACT_APP_ENABLE_GITHUB_INTEGRATION=true
REACT_APP_ENABLE_DRIVE_INTEGRATION=true
REACT_APP_MAX_FILE_SIZE=10485760
REACT_APP_SUPPORTED_FILE_TYPES=.pdf,.txt,.docx,.md,.py,.js,.ts,.json,.csv,.xlsx

# ======================
# DEVELOPMENT SETTINGS
# ======================
REACT_APP_DEV_MODE=true
REACT_APP_DEBUG_LOGGING=false
REACT_APP_SHOW_DEV_TOOLS=true
REACT_APP_HOT_RELOAD=true

# ======================
# SECURITY & AUTHENTICATION
# ======================
REACT_APP_JWT_SECRET=your_jwt_secret_key_here
REACT_APP_SESSION_TIMEOUT=86400
REACT_APP_ENABLE_GUEST_MODE=true
REACT_APP_REQUIRE_EMAIL_VERIFICATION=false

# ======================
# ANALYTICS & MONITORING
# ======================
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ANALYTICS_ID=your_analytics_id_here
REACT_APP_ERROR_REPORTING=true
REACT_APP_PERFORMANCE_MONITORING=true

# ======================
# INTEGRATIONS
# ======================
# GitHub Integration
REACT_APP_GITHUB_CLIENT_ID=your_github_client_id_here
REACT_APP_GITHUB_CLIENT_SECRET=your_github_client_secret_here

# Google Drive Integration  
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id_here
REACT_APP_GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# OneDrive Integration
REACT_APP_ONEDRIVE_CLIENT_ID=your_onedrive_client_id_here
REACT_APP_ONEDRIVE_CLIENT_SECRET=your_onedrive_client_secret_here

# ======================
# ADVANCED FEATURES
# ======================
# Quantum Computing
REACT_APP_QUANTUM_SIMULATOR_URL=http://localhost:8001
REACT_APP_QUANTUM_MAX_QUBITS=32

# Time Crystals
REACT_APP_TIME_CRYSTAL_ENABLED=true
REACT_APP_TIME_CRYSTAL_SYNC_INTERVAL=1000

# Neuromorphic AI
REACT_APP_NEUROMORPHIC_ENABLED=true
REACT_APP_SPIKING_NETWORK_SIZE=1000

# Blockchain Integration
REACT_APP_BLOCKCHAIN_ENABLED=false
REACT_APP_WEB3_PROVIDER_URL=http://localhost:8545

# ======================
# STORAGE & PERSISTENCE
# ======================
REACT_APP_LOCAL_STORAGE_PREFIX=aetherium_
REACT_APP_CHAT_HISTORY_LIMIT=1000
REACT_APP_AUTO_SAVE_INTERVAL=5000
REACT_APP_ENABLE_CLOUD_SYNC=false

# ======================
# UI/UX PREFERENCES
# ======================
REACT_APP_DEFAULT_THEME=dark
REACT_APP_ENABLE_ANIMATIONS=true
REACT_APP_SIDEBAR_WIDTH=280
REACT_APP_CHAT_BUBBLE_ANIMATION=true
REACT_APP_QUANTUM_EFFECTS=true

# ======================
# PERFORMANCE SETTINGS
# ======================
REACT_APP_WEBSOCKET_RECONNECT_INTERVAL=3000
REACT_APP_API_RETRY_ATTEMPTS=3
REACT_APP_LAZY_LOADING=true
REACT_APP_CHUNK_SIZE=1024

# ======================
# DEPLOYMENT SETTINGS
# ======================
REACT_APP_BUILD_VERSION=1.0.0
REACT_APP_BUILD_DATE=2025-01-06
REACT_APP_DEPLOYMENT_ENV=development